lst1=[]
while True:
    num=int(input("Enter a number:"))
    lst1.append(num)
    if num==0:
        break
print("Mz element in the list is:",max(lst1))
print("min element in the list is:",min(lst1))
for i in lst1:
    if(i>=0) and (i<=9):
        print("single digit:")
    elif(i>=10)and (i<=99):
        print("double digit:")
    else:
        if(i>=100) and (i<=999):
            print("3 digit:")
    
