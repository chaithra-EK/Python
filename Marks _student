
def total_grade(eng,sciencetheory,sciencepractical,maths):
    total1=eng+sciencetheory+sciencepractical+maths
    science=sciencetheory+sciencepractical
    percentage=(total1/total)*100
    if(percentage>=90):
        print("Grade A")
    elif(percentage>=75)and(percentage<90):
        print("Grade B")
    elif(eng<25)or(sciencetheory<25)or(sciencepractical<8)or(maths<35)or(science<35):
        print("Fail")
        
    else:
        print("average")







def marks_scored():
    eng=int(input("Enter the marks in english:"))
    sciencetheory=int(input("enter the marks scored in science theory:"))
    sciencepractical=int(input("enter the marks scored in science practical:"))
    maths=int(input("Enter the marks scored in maths:"))
    return eng,sciencetheory,sciencepractical,maths



