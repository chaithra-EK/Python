title=input("enter the title\t")
header=input("enter the header")
body=input("enter the first paragraph")
body1=input("enter the second paragraph")

print("\t\t\t\t Webpage.html")
print("<html>\n\t<head>\n\t\t<title>")
print("\n\t\t\t",title)
print("\n\t\t</title>")
print("\n\t</head>")
print("\n\t<body>")

print("\n \t\t<h1>", header,"</h1>")
print("\n\t\t<p>",body,"\n\t\t</p>")
print("\n\t\t <p>",body1,"\n\t\t</p>")
print("\n\t</body>")
print("</html>")
